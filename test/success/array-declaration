// basic array of bools
let array = [ true, true, false ];
print array;

// 2d array (10x10) array of falses
let false_array = [ 10 false ];
let array = [ 10 false_array ];
print array;

// empty array of string array type
let array: [ str ] = [ 0 '' ];
print array;

// array of arrays
let array: [ str ] = [ 'str' ];
print array;

// array of array of functions
let fun = fn () => str return 'hi'; ;
let array: [[ () => str ]] = [ [ fun, fun ], [ fun ] ];
print array;
