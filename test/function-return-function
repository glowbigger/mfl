let returnAdder = fn () => () => num {
  let counter = 0;
  return fn () => num { 
    counter = counter + 1;
    return counter;
  };
};

let adder = returnAdder();
print adder(); // 1

let adder = returnAdder();
print adder(); // 1
print adder(); // 2

let adder2 = returnAdder();

print adder2(); // 1
print adder2(); // 2
print adder2(); // 3

print adder(); // 3
