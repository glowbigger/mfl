let a: str = "global a";
let b: str = "global b";
let c: str = "global c";
{
  let a: str = "outer a";
  let b: str = "outer b";
  {
    let a: str = "inner a";
    print a;
    print b;
    print c;
  }
  print a;
  print b;
  print c;
}
print a;
print b;
print c;

// invalid declarations within scopes
let a: num = 5;
{
  let a: str = "imastring";
  a = 5;
}
